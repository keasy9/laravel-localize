{
  "name": "keasy9/laravel-localize",
  "description": "A small Laravel package that helps with site localization.",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "keasy9",
      "email": "keasy9@gmail.com",
      "homepage": "https://github.com/keasy9"
    }
  ],
  "require": {
    "php": "^7.3 || ^8.0",
    "laravel/framework": ">=8.0",
    "keasy9/laravel-composite-pk": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Keasy9\\Localize\\": "src"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Keasy9\\Localize\\Providers\\LocalizeServiceProvider"
      ],
      "aliases": {
        "Localize": "Keasy9\\Localize\\Facades\\Localize"
      }
    }
  }
}